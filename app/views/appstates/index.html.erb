<p id="notice"><%= notice %></p>

<h1>Appstates</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @appstates.each do |appstate| %>
      <tr>
        <td><%= appstate.description %></td>
        <td><%= link_to 'Show', appstate %></td>
        <td><%= link_to 'Edit', edit_appstate_path(appstate) %></td>
        <td><%= link_to 'Destroy', appstate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Appstate', new_appstate_path %>
